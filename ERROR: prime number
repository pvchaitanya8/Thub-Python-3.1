def prime(b):
    for i in range(2,b):
        if(b%i==0):
            return False
    return True


def first_n_pair_of_prime_numbers(n):
    print(2)
    print(3)
    for i in range(1,n):
        print()
        print("PAIR:")
        a=((6*i)-1)
        b=((6*i)+1)
        print(a, prime(a))
        print(b, prime(b))
        
        

x=int(input("Enter number: "))
first_n_pair_of_prime_numbers(x)

